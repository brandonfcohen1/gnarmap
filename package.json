{
  "name": "gnarmap",
  "private": true,
  "workspaces": ["apps/*", "packages/*"],
  "scripts": {
    "dev": "bun run --cwd apps/web dev",
    "build": "bun run --cwd apps/web build",
    "lint": "bun run --cwd apps/web lint",
    "typecheck": "bun run --cwd apps/web typecheck",
    "pipeline:build": "cd packages/pipeline && cargo build --release",
    "pipeline:daily": "cd packages/pipeline && ./target/release/snodas-pipeline daily",
    "pipeline:backfill": "cd packages/pipeline && ./target/release/snodas-pipeline backfill",
    "pipeline:build-zarr": "cd packages/pipeline && ./target/release/snodas-pipeline build-zarr"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "fflate": "^0.8.2",
    "recharts": "^3.6.0"
  }
}
